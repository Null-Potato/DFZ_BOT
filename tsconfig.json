{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",
 
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
	"outDir": "./built",
	"allowJs": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
	"allowUnreachableCode": false
  },
  "include": ["./src/**/*", "./website/**/*"]
}